{
  "name": "@clownpack/esbuild-preset-env",
  "version": "0.0.0",
  "description": "a esbuild plugin",
  "keywords": [],
  "author": "Binbiubiubiu <2632003786@qq.com>",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index"
      ]
    }
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./helper": {
      "types": "./dist/helper.d.ts",
      "default": "./dist/helper.js"
    },
    "./types": "./dist/types.d.ts",
    "./*": "./dist/*"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc"
  },
  "packageManager": "pnpm@8.1.0",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Binbiubiubiu/clownpack.git"
  },
  "bugs": {
    "url": "https://github.com/Binbiubiubiu/clownpack/issues"
  },
  "homepage": "https://github.com/Binbiubiubiu/clownpack#readme",
  "dependencies": {
    "@clownpack/helper": "workspace:*"
  },
  "devDependencies": {
    "esbuild": "0.17.15"
  }
}
