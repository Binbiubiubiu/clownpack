{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"],
      "outputMode": "new-only"
    },
    "dev": {
      "cache": false
    },
    "test": {
      "inputs": ["__tests__"],
      "outputs": ["coverage"],
      "outputMode": "new-only"
    }
  },
  "globalDependencies": ["tsconfig.base.json"]
}
